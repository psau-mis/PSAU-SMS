<odoo>
    <data noupdate="1">
       <record id="seq_cashier_invoice" model="ir.sequence">
           <field name="name">Cashier Invoice</field>
           <field name="code">esmis.cashier</field>
           <field name="prefix">CI</field>
           <field name="padding">5</field>
           <field name="company_id" eval="False"/>
       </record>

       <record id="seq_account_id" model="ir.sequence">
           <field name="name">Account ID</field>
           <field name="code">esmis.account.id</field>
           <field name="prefix"></field>
           <field name="padding">0</field>
           <field name="company_id" eval="False"/>
       </record>

     
       <record id="seq_cashier_or_id" model="ir.sequence">
           <field name="name">OR</field>
           <field name="code">esmis.cashier.or</field>
           <field name="prefix"></field>
           <field name="padding">0</field>
           <field name="company_id" eval="False"/>
       </record>
    </data>
</odoo>