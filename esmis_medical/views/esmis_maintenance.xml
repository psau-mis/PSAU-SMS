<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       
       <!-- _________________________________procedure types______________________________________________-->
        <record id="view_esmis_procedure_type_list" model="ir.ui.view">
            <field name="name">esmis.procedure.type.list</field>
            <field name="model">esmis.procedure.type</field>
            <field name="arch" type="xml">
                <tree sample="1" string="Procedure Type" editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>
   
        <record id="view_esmis_procedure_type_search" model="ir.ui.view">
            <field name="name">esmis.procedure.type.search</field>
            <field name="model">esmis.procedure.type</field>
            <field name="arch" type="xml">
                <search string="Procedure Type">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="action_esmis_procedure_type_form" model="ir.actions.act_window">
            <field name="name">Procedure Type</field>
            <field name="res_model">esmis.procedure.type</field>
            <field name="view_mode">tree,kanban</field>
            <field name="search_view_id" ref="view_esmis_procedure_type_search"/>
            <field name="view_id" ref="view_esmis_procedure_type_list"/>
            <field name="context"></field>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Click to add a Procedure Type.
              </p><p>
                Procedure Type List
              </p>
            </field>
        </record>

        <menuitem 
        id="menu_esmis_procedure_type" 
        action="action_esmis_procedure_type_form"
        parent="esmis_medical.menu_medical_config"
        name="Procedure Types"   
        sequence="10"/>


